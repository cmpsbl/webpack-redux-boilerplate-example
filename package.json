{
  "name": "webpack-redux-boilerplate-example",
  "version": "0.3.0",
  "description": "Webpack Redux Boilerplate Example",
  "main": "index.js",
  "repository": "",
  "scripts": {
    "clean": "rimraf dist",
    "start": "better-npm-run start",
    "compile": "better-npm-run compile",
    "deploy": "better-npm-run deploy",
    "lint": "eslint src",
    "test": "node_modules/mocha/bin/mocha --compilers js:babel-core/register --recursive"
  },
  "betterScripts": {
    "start": {
      "command": "nodemon --exec babel-node config/server",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "development"
      }
    },
    "compile": {
      "command": "babel-node config/compiler",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.7.2",
    "better-npm-run": "0.0.8",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-import-resolver-webpack": "^0.2.4",
    "eslint-plugin-import": "^1.6.0",
    "eslint-plugin-jsx-a11y": "^1.0.3",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.16.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.2",
    "react-addons-test-utils": "^15.0.1",
    "react-hot-loader": "^3.0.0-beta.1",
    "redbox-react": "^1.2.3",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "lodash": "^4.11.1",
    "react": "^15.0.1",
    "react-css-modules": "^3.7.6",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "redux": "^3.5.2"
  }
}
